{
  "name": "@vibes.diy/vibesbox-tests",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "description": "Tests for @vibes.diy/vibesbox Cloudflare Worker",
  "repository": {
    "type": "git",
    "url": "https://github.com/VibesDIY/vibes.diy.git"
  },
  "homepage": "https://github.com/VibesDIY/vibes.diy",
  "bugs": {
    "url": "https://github.com/VibesDIY/vibes.diy/issues"
  },
  "scripts": {
    "test": "vitest --config vitest.config.ts run",
    "test:watch": "vitest --config vitest.config.ts",
    "test:coverage": "vitest --config vitest.config.ts run --coverage",
    "format": "prettier --write \"**/*.{js,ts,json,css,md}\"",
