  "dependencies": {
    "@vibes.diy/vibesbox": "workspace:*"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240925.0",
    "@types/node": "^24.7.2",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
